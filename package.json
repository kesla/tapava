{
  "name": "tapava",
  "version": "1.0.1",
  "description": "the syntax of ava, run through tape",
  "main": "dist/index.js",
  "scripts": {
    "lint": "semistandard | snazzy",
    "test:browser": "browserify test.js -t babelify -d | devtool -c -h --bf --no-nt | faucet",
    "test:node": "nyc babel-tape-runner test.js | faucet",
    "test": "npm run test:node && npm run test:browser && npm run lint",
    "prepublish": "mkdir -p dist && babel lib --out-dir dist"
  },
  "author": "David BjÃ¶rklund <david.bjorklund@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "tape": "^4.4.0",
    "tape-catch": "^1.0.5"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.8.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-tape-runner": "^2.0.1",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "concat-stream": "^1.5.1",
    "devtool": "^1.11.2",
    "faucet": "0.0.1",
    "nyc": "^6.4.4",
    "semistandard": "^8.0.0",
    "snazzy": "^4.0.0",
    "tap-parser": "^1.2.2"
  },
  "keywords": [
    "ava",
    "tape"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kesla/tapava.git"
  },
  "bugs": {
    "url": "https://github.com/kesla/tapava/issues"
  },
  "homepage": "https://github.com/kesla/tapava#readme"
}
